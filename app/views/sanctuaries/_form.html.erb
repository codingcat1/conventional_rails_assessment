<%= render("layouts/errors", :object => @sanctuary) %>


<%= form_for(@sanctuary) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>
  <%= f.submit(:class => "btn btn-primary btn-sm") %>
<% end %>
